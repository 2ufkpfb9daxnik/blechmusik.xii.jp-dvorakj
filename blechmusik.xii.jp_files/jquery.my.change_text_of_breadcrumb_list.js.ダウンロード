$(function() {
    $('ul.breadcrumb li').each(function() {
        var text = $(this).text();

        console.log(text);

        switch (text) {
            case "d":
                $(this).text("資料集");
                break;

            case "about":
                $(this).remove();
                break;

            case "design":
                $(this).remove();
                break;

                // default:

                //     break;

        }
    });


    $('ul.breadcrumb li:last').addClass("active");
});


